{
  "name": "terminal-transcript-saver",
  "displayName": "Terminal Transcript Saver",
  "description": "Save terminal transcripts with Cmd+S",
  "version": "0.1.0",
  "publisher": "kylermjensen",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:terminalTranscriptSaver.save"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "terminalTranscriptSaver.save",
        "title": "Save Terminal Transcript"
      }
    ],
    "keybindings": [
      {
        "command": "terminalTranscriptSaver.save",
        "key": "cmd+shift+s",
        "mac": "cmd+shift+s",
        "when": "terminalFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.80.0",
    "typescript": "^5.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kylermjensen/vscode-terminal-saver"
  },
  "license": "MIT"
}
